package bt;
import java.util.Scanner;
public class bai6 {
public static void main(String[] args) {
	int a;
	int b;
	// ax + b = 0;
	Scanner sc = new Scanner(System.in);
	a = sc.nextInt();
	b = sc.nextInt();
	double x;
	if(a == 0) {
		if(b != 0) {
			System.out.println("vo nghiem");
		}
		else {
			System.out.println("0");
		}
	}
	else {
		x = -(double)b/a;
		System.out.println(x);
	}
}
}
